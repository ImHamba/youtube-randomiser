// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

model User {
  userId       Int    @id @default(autoincrement())
  email        String @unique
  passwordHash String
}

model SavedMixes {
  mixId    Int    @id @default(autoincrement())
  userId   Int
  mixData String
}
